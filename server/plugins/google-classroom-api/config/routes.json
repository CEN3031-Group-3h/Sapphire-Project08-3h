{
  "routes": [
    {
      "method": "GET",
      "path": "/courses",
      "handler": "classroom.list",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/courses/:id",
      "handler": "classroom.get",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/courses/:id",
      "handler": "classroom.create",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PATCH",
      "path": "/google-classroom/:courseId/:courseWorkId/:studentId",
      "handler": "classroom.updateStudentGrade",
      "config": {
        "policies": []
      }
    }
  ]
}
